PKG_CPPFLAGS=-I. -I../inst/include -DRANN

LIBNAME=libRANN2.a

$(SHLIB): ../inst/lib/$(LIBNAME)
../inst/lib/$(LIBNAME): ANN.o bd_fix_rad_search.o bd_pr_search.o bd_search.o bd_tree.o brute.o kd_dump.o kd_fix_rad_search.o kd_pr_search.o kd_search.o kd_split.o kd_tree.o kd_util.o WANN.o
	(mkdir -p ../inst/lib; $(AR) rcs $@ $^)
